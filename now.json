{
  "version": 2,
  "builds": [
    {
      "src": "website/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build/backend-fundamentals"
      }
    },
    {
      "src": "website/package.json"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "website/$1"
    }
  ]
}
